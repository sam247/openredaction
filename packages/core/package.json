{
  "name": "openredaction",
  "version": "1.0.4",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/index.js",
    "dist/index.mjs",
    "dist/index.d.ts",
    "dist/index.d.mts",
    "dist/index.cli.js",
    "dist/cli/"
  ],
  "scripts": {
    "build": "tsdown && node scripts/dual-types.js",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench --run",
    "bench:watch": "vitest bench",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "license": "MIT",
  "peerDependencies": {
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "tesseract.js": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "pdf-parse": {
      "optional": true
    },
    "mammoth": {
      "optional": true
    },
    "tesseract.js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.6",
    "@vitest/coverage-v8": "^1.0.4",
    "tsdown": "^0.20.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
