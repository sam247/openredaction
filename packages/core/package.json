{
  "name": "openredaction",
  "version": "1.0.6",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/index.js",
    "dist/index.mjs",
    "dist/index.d.ts",
    "!dist/*.cli.js",
    "!dist/cli/",
    "!dist/chunk-*.mjs",
    "!dist/chunk-*.mjs.map",
    "!dist/workers-*.mjs",
    "!dist/document-*.mjs",
    "!dist/HealthCheck-*.mjs",
    "!README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench --run",
    "bench:watch": "vitest bench",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "license": "MIT",
  "peerDependencies": {
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "tesseract.js": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "pdf-parse": {
      "optional": true
    },
    "mammoth": {
      "optional": true
    },
    "tesseract.js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.6",
    "@vitest/coverage-v8": "^1.0.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
